mylsq=function(x,y){
ssxx=sum((x-mean(x))^2 )
ssxy=sum((x-mean(x))*(y-mean(y)))
b1hat=ssxy/ssxx
b0hat=mean(y)-b1hat*mean(x)
return(list(b0hat=b0hat,b1hat=b1hat))
}
